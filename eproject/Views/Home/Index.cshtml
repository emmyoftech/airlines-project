@{
    ViewData["Title"] = "Home Page";
    ViewData["Script"] = "/js/Home/index.js";
    ViewData["Style"] = "/css/Home/index.css";
    ViewData["CarStyle"] = "/lib/plugins/owl-carousel/owl.carousel.min.css";
    ViewData["CarThemeStyle"] = "/lib/plugins/owl-carousel/owl.theme.default.css";
    ViewData["CarScript"] = "/lib/plugins/owl-carousel/owl.carousel.min.js";
    ViewData["Jquery"] = "/lib/plugins/owl-carousel/jquery.min.js";
